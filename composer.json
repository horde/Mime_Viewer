{
    "name": "horde/mime-viewer",
    "description": "MIME viewer library",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_Mime_Viewer",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        },
        {
            "name": "Michael Slusarz",
            "email": "slusarz@horde.org",
            "role": "lead"
        }
    ],
    "version": "2.2.4",
    "time": "2022-03-02",
    "repositories": [
        {
            "type": "pear",
            "url": "https://pear.horde.org"
        }
    ],
    "require": {
        "php": "^5.3 || ^7",
        "pear-pear.horde.org/Horde_Browser": "^2",
        "pear-pear.horde.org/Horde_Compress": "^2",
        "pear-pear.horde.org/Horde_Exception": "^2",
        "pear-pear.horde.org/Horde_Mime": "^2",
        "pear-pear.horde.org/Horde_Text_Filter": "^2",
        "pear-pear.horde.org/Horde_Text_Flowed": "^2",
        "pear-pear.horde.org/Horde_Util": "^2",
        "pear-pear.horde.org/Horde_Translation": "^2.2",
        "ext-xml": "*"
    },
    "suggest": {
        "pear-pear.php.net/Net_DNS2": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-xsl": "*"
    },
    "replace": {
        "pear-pear.horde.org/Horde_Mime_Viewer": "2.*",
        "pear-horde/Horde_Mime_Viewer": "2.*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Mime_Viewer": "lib/"
        }
    }
}
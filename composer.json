{
    "name": "horde/mime_viewer",
    "description": "MIME viewer library",
    "type": "horde-library",
    "homepage": "https://www.horde.org/libraries/Horde_Mime_Viewer",
    "license": "LGPL-2.1-only",
    "authors": [
        {
            "name": "Michael Slusarz",
            "email": "slusarz@horde.org",
            "role": "lead"
        }
    ],
    "time": "2022-03-02",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com"
        }
    ],
    "require": {
        "horde/horde-installer-plugin": "dev-FRAMEWORK_6_0 || *",
        "php": "^7.4 || ^8",
        "horde/browser": "^3 || dev-FRAMEWORK_6_0",
        "horde/compress": "^3 || dev-FRAMEWORK_6_0",
        "horde/exception": "^3 || dev-FRAMEWORK_6_0",
        "horde/mime": "^3 || dev-FRAMEWORK_6_0",
        "horde/text_filter": "^3 || dev-FRAMEWORK_6_0",
        "horde/text_flowed": "^3 || dev-FRAMEWORK_6_0",
        "horde/util": "^3 || dev-FRAMEWORK_6_0",
        "horde/translation": "^3 || dev-FRAMEWORK_6_0",
        "ext-xml": "*"
    },
    "require-dev": {
        "horde/test": "^3 || dev-FRAMEWORK_6_0"
    },
    "suggest": {
        "pear/net_dns2": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-xsl": "*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Mime_Viewer": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Mime\\Viewer\\Test\\": "test/"
        }
    },
    "minimum-stability": "dev"
}

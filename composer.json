{
    "name": "horde/horde-mime-viewer",
    "description": "Horde MIME Viewer Library",
    "type": "library",
    "homepage": "https://pear.horde.org",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Michael Slusarz",
            "email": "slusarz@horde.org",
            "role": "lead"
        }
    ],
    "version": "2.2.2",
    "time": "2016-09-06",
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.horde.org"
        }
    ],
    "require": {
        "php": "^5.3 || ^7",
        "pear-pear.horde.org/Horde_Browser": "^2@stable",
        "pear-pear.horde.org/Horde_Compress": "^2@stable",
        "pear-pear.horde.org/Horde_Exception": "^2@stable",
        "pear-pear.horde.org/Horde_Mime": "^2@stable",
        "pear-pear.horde.org/Horde_Text_Filter": "^2@stable",
        "pear-pear.horde.org/Horde_Text_Flowed": "^2@stable",
        "pear-pear.horde.org/Horde_Util": "^2@stable",
        "pear-pear.horde.org/Horde_Translation": "^2.2@stable",
        "ext-xml": "*"
    },
    "suggest": {
        "pear-pear.php.net/Net_DNS2": "*"
    },
    "replace": {
        "pear-pear.horde.org/Horde_Mime_Viewer": "2.*",
        "pear-horde/Horde_Mime_Viewer": "2.*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Mime_Viewer": "lib/"
        }
    }
}
